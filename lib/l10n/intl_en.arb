    },
    "emailHint": "Email",
    "@emailHint": {
        "description":"Sets the hint for the email field."
    },
    "passwordHint": "Password",
    "@passwordHint": {
        "description":"Sets the hint for the password field."
    },
    "confirmPasswordHint": "Confirm Password",
    "@confirmPasswordHint": {
        "description":"Sets the hint for the confirm password field."
    },
    "registrationDialogContent": "Registration was successfull...Username: {username} Id: {id} Email:{email}",
    "@registrationDialogContent": {
        "description":"Sets the alert dialog content after successfully registering.",
        "placeholders": {
          "username": {
            "type": "String",
            "example": "user"
          },
          "id": {
            "type": "int",
            "format": "decimalPattern",
            "example": "123"
          },
          "email": {
            "type": "String",
            "example": "user@mail.com"
          }
        }
    },
    "unauthorizedError": "User is not authorized",
    "@unauthorizedError": {
        "description":"Sets the error message when the user is not authorized"
    },
    "refreshAccessTokenFailure": "Couldn't refresh access token...try to log-in again",
    "@refreshAccessTokenFailure": {
        "description":"Sets the error message when the application fails to refresh the access token"
    }
